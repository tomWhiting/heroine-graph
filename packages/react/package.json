{
  "name": "@heroine-graph/react",
  "version": "0.1.0",
  "description": "React wrapper for Heroine Graph - high-performance graph visualization",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@heroine-graph/core": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  },
  "keywords": [
    "react",
    "graph",
    "visualization",
    "webgpu",
    "heroine-graph"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/heroine-graph/heroine-graph.git",
    "directory": "packages/react"
  },
  "sideEffects": false
}
